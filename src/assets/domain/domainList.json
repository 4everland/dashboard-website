{
  "list": [
    {
      "name": "ENS",
      "key": "ens",
      "network": "function(domain){return 'https://' +  domain + '.limo'}",
      "host": "https://app.ens.domains",
      "rule": ".+.eth$"
    },
    {
      "name": "SNS",
      "key": "sns",
      "network": "function(domain){return 'https://' + domain.split('.')[0] + '.4sol.xyz'}",
      "host": "https://naming.bonfida.org",
      "rule": ".+.sol$"
    }
  ]
}
