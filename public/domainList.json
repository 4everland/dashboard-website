{
  "list": [
    {
      "name": "ENS",
      "key": "ens",
      "network": "function(domain){return 'https://' +  domain + '.limo'}",
      "host": "https://app.ens.domains",
      "rule": ".+.eth$",
      "placeholder": "ENS Domain",
      "icon": "ens.svg",
      "display": "true"
    },
    {
      "name": "SNS",
      "key": "sns",
      "network": "function(domain){return 'https://' + domain.split('.')[0] + '.4sol.xyz'}",
      "host": "https://naming.bonfida.org",
      "rule": ".+.sol$",
      "placeholder": "SNS Domain",
      "icon": "sns.svg",
      "display": "true",
      "register": "true",
      "registerLink": "https://sns.id/?ref=4everland"
    },
    {
      "name": "SPACE ID",
      "key": "spaceid",
      "network": "function(domain){return 'https://' + domain +'.bnb'}",
      "host": "https://bnb.me/flow/ipfs",
      "rule": ".+.bnb$",
      "placeholder": "SPACE ID",
      "icon": "spaceid.svg",
      "display": "true"
    }
  ]
}
